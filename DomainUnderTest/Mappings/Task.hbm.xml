<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DomainUnderTest"
                   namespace="DomainUnderTest">

  <class name="Task">
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="Title" />
    <property name="Description" />
    <property name="Start" />
    <property name="End" />
  </class>

</hibernate-mapping>